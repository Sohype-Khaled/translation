{
    "name": "codtail/translation",
    "description": "An elegant laravel model translation tool",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sohype Khaled",
            "email": "sohype.mop@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Codtail\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codtail\\Translation\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Translator": "Codtail\\Translation\\Translator"
            },
            "providers": [
                "Codtail\\Translation\\TranslationServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0@dev"
    }
}
